    "# %load \"./preprocessing.py\"\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "def clean_data(df):\n",
    "    print(df.head(3))\n",
    "    df = df.drop(['team', 'opp'], axis=1)\n",
    "    df = df.dropna()\n",
    "    \n",
    "    print(\"\\n Checking for Null Values.\")\n",
    "    print(df.isnull().sum())\n",
    "    print(df.shape)\n",
    "    print(df.dtypes)\n",
    "    \n",
    "    #start by dropping player, team, matchID, opp\n",
    "    df = df.drop(['ID', 'GAME_ID', 'MATCHUP', 'LOCATION', 'GAME_CLOCK', 'PTS', 'SHOT_RESULT','player_name',\n",
    "                            'CLOSEST_DEFENDER1', 'made', 'time_remaining', 'shot_type', 'player', 'CLOSEST_DEFENDER', \n",
    "                            'player_id', 'CLOSEST_DEFENDER_PLAYER_ID'], axis=1)\n",
    "    \n",
    "    X = df.drop('FGM', axis=1)\n",
    "    y = df['FGM']\n",
    "    \n",
    "    X_num = X.select_dtypes(include='number')\n",
    "    X_cat = X.select_dtypes(exclude='number')\n",
    "    X_num = StandardScaler().set_output(transform='pandas').fit_transform(X_num)\n",
    "    X_cat = pd.get_dummies(X_cat, drop_first=True)\n",
    "    X = pd.concat([X_num,X_cat],axis=1)\n",
    "    \n",
    "    return X, y\n"
   ]
